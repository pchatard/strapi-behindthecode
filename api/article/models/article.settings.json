{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "name": "article",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "content": {
      "type": "richtext",
      "required": true
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false
    },
    "subjects": {
      "collection": "subject"
    },
    "resources": {
      "collection": "resource",
      "via": "articles"
    },
    "category": {
      "via": "articles",
      "model": "category"
    },
    "preview": {
      "type": "text",
      "required": true
    }
  }
}
